FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rbgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
    type            fixedJump;
    patchType       cyclic;
    jump            uniform -5.12e-06;
    value           $internalField;
    }
    outlet
    {
        type            fixedJump;
    patchType       cyclic;
    value           $internalField;
    }
    top
    {
    type  fixedValue;
    value uniform 0;
    }
    walls
    {
    type  zeroGradient;
    }
    frontAndBackPlanes
    {
        type            empty;
    }
}


